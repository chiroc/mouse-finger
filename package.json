{
  "name": "mouse-finger",
  "version": "1.0.0",
  "description": "Mouse Finger",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "npm rebuild --runtime=electron --target=2.0.0 --disturl=https://atom.io/download/atom-shell --abi=57",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Chiroc Cai",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^2.0.0",
    "electron-builder": "^20.28.4",
    "electron-packager": "^12.2.0"
  },
  "dependencies": {
    "charm": "^1.0.2",
    "iohook": "^0.2.0",
    "robotjs": "^0.5.1",
    "ffi": "^2.2.0"
  },
  "iohook": {
    "targets": [
      "node-57",
      "electron-57"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  },
  "build": {
    "appId": "com.mouse-finger.app",
    "win": {
      "target": "zip",
      "icon": "assets/logo.ico"
    }
  }
}
